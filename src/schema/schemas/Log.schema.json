{
    "$id": "https://microsoft.com/is/Log",
    "$schema": "http://json-schema.org/draft-07/schema",
    "description": "Industry Solutions graph log record",
    "type": "object",
    "required": [ "month", "id", "tag", "docIds", "documents"],
    "properties": {
        "month": {
            "type": "string",
            "pattern": "^([0-9]{4})-(1[0-2]|0[1-9])(/[a-z]+)*$"
        },
        "id": {
            "type": "string"
        },
        "tag": {
            "$ref": "core#/definitions/tag"
        },
        "docIds": {
            "type": "array",
            "contains": {
                "type": "string"
            },
            "uniqueItems": true,
            "minItems": 1
        },
        "documents": {
            "type": "array",
            "contains": {
                "type": "object"
            },
            "uniqueItems": true,
            "minItems": 1
        }
    }
}
